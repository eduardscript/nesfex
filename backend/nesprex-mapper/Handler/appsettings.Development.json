{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Service": {
    "Interval": "00:15:00",
    "PingInterval": "00:00:30"
  },
  "Kafka": {
    "BootstrapServer": "localhost:9092",
    "Producer": {
      "ClientId": "nesprex-mapper",
      "Topics": [
        {
          "Name": "technologies",
          "Topic": "nesprex.products",
          "EmptyProduce": true
        },
        {
          "Name": "images",
          "Topic": "nesprex.images",
          "EmptyProduce": true
        }
      ]
    }
  },
  "ImageUrl": "https://www.nespresso.com"
}
